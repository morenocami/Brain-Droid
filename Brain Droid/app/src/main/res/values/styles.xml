    <!--
           Base application theme, dependent on API level. This theme is replaced
           by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
       -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- style for edittext -->
    <style name="edittextlayout" parent="@style/layout_height_weight.horizontal_weight">
        <item name="android:layout_margin">10dp</item>
    </style>

    <!-- Layout Styles -->
    <style name="layout_fill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="layout_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="layout_height_wrap">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
    </style>

    <style name="layout_width_wrap">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="layout_height_weight">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="layout_height_weight_widthwrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="layout_width_weight">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">0dp</item>
    </style>

    <style name="layout_width_weight_heightwrap">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
    </style>

    <style name="layout_fill.horizontal_weightsum">
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">10</item>
    </style>

    <style name="layout_fill.vertical_weightsum">
        <item name="android:orientation">vertical</item>
        <item name="android:weightSum">10</item>
    </style>

    <style name="layout_width_weight.vertical_weightsum">
        <item name="android:orientation">vertical</item>
        <item name="android:weightSum">10</item>
    </style>

    <style name="layout_height_weight.vertical_weightsum">
        <item name="android:orientation">vertical</item>
        <item name="android:weightSum">10</item>
    </style>

    <style name="layout_height_weight.horizontal_weight">
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">10</item>
    </style>

    <style name="layout_wrap.horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

