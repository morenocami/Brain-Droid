        <item name="android:inputType">textPassword</item>
        <item name="android:hint">Password</item>
        <item name="android:layout_marginRight">@dimen/layout_margin50dp</item>
        <item name="android:layout_marginLeft">@dimen/layout_margin50dp</item>
        <item name="android:layout_marginBottom">@dimen/layout_margin10dp</item>
        <item name="android:layout_marginTop">@dimen/layout_margin10dp</item>
    </style>

    <style name="edittext_height_wrap_email_login" parent="@style/layout_height_wrap">
        <item name="android:inputType">textEmailAddress</item>
        <item name="android:hint">Email-Id</item>
        <item name="android:layout_marginRight">@dimen/layout_margin50dp</item>
        <item name="android:layout_marginLeft">@dimen/layout_margin50dp</item>
        <item name="android:layout_marginBottom">@dimen/layout_margin10dp</item>
        <item name="android:layout_marginTop">@dimen/layout_margin10dp</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="edittext_height_wrap_email_password" parent="@style/layout_height_wrap">
        <item name="android:inputType">textPassword</item>
        <item name="android:hint">Password</item>
        <item name="android:layout_marginRight">@dimen/layout_margin50dp</item>
        <item name="android:layout_marginLeft">@dimen/layout_margin50dp</item>
        <item name="android:layout_marginBottom">@dimen/layout_margin10dp</item>
        <item name="android:layout_marginTop">@dimen/layout_margin10dp</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:textColor">@color/white</item>
